{
  "version": 2,
  "functions": {
    "run.py": {
      "runtime": "python3.9",
      "includeFiles": "requirements.txt"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "PIP_NO_CACHE_DIR": "off",
    "PIP_DISABLE_PIP_VERSION_CHECK": "on",
    "PIP_DEFAULT_TIMEOUT": "100"
  }
}
