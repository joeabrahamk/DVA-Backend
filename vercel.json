{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["requirements.txt"],
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "build": {
    "env": {
      "PIP_NO_CACHE_DIR": "off",
      "PIP_DISABLE_PIP_VERSION_CHECK": "on",
      "PIP_DEFAULT_TIMEOUT": "100"
    },
    "commands": [
      "pip install -r requirements.txt"
    ]
  }
}
